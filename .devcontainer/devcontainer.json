{
  "name": "StudioCMS Workspace",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "postCreateCommand": "proto install && pnpm install",
  "waitFor": "postCreateCommand",
  "customizations": {
    "vscode": {
      "extensions": [
        "astro-build.astro-vscode",
        "astro-build.houston",
        "biomejs.biome",
        "eamodio.gitlens",
        "redhat.vscode-yaml"
      ]
    }
  }
}
