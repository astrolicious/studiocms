---
// export const partial = true; - Disabled due to regression | See @ https://github.com/withastro/astro/issues/11103
import { StudioCMSRenderer } from "studiocms:components";

const query = Astro.url.searchParams.get("content");
const preQuery = Astro.url.searchParams.get("preload-content");
const noContent = "# No content found, Either your Page content is empty or you have not updated the entry.";
const content = query ? query : (preQuery ? preQuery : noContent);
---
<StudioCMSRenderer content={content} />