---
export const partial = true;
import { StudioCMSRenderer } from "studiocms:components";

const query = Astro.url.searchParams.get("content");
const preQuery = Astro.url.searchParams.get("preload-content");

let content = null;

if (query && query !== "null") {
    content = query;
} else if (preQuery && preQuery !== "null") {
    content = preQuery;
} else {
    content = "No content to display";
}
---
<StudioCMSRenderer content={content} />