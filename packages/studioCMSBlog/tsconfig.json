{
  "extends": "astro/tsconfigs/strictest",
  "files": [],
  "compilerOptions": {
    "outDir": "../../.moon/cache/types/packages/studioCMSBlog",
    "paths": {
      "@astrolicious/studiocms": [
        "../studioCMS/src/index.ts"
      ],
      "@astrolicious/studiocms/*": [
        "../studioCMS/src/*"
      ],
      "node-playground/*": [
        "../../playgrounds/node/src/*"
      ]
    }
  },
  "references": [
    {
      "path": "../studioCMS"
    },
    {
      "path": "../../playgrounds/node"
    }
  ],
  "include": [
    "../../playgrounds/node/**/*",
    "../../playgrounds/node/.astro/**/*",
    "../studioCMS/**/*",
    "./**/*"
  ]
}
