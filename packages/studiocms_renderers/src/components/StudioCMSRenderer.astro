---
import rendererConfig from 'studiocms:renderer/config';
import type { StudioCMSRendererConfig } from '@studiocms/core/schemas';
import { contentRenderer } from '../lib/contentRenderer';

type Props = {
	content: string;
	renderer?: StudioCMSRendererConfig['renderer'];
};

const { content, renderer = rendererConfig.renderer } = Astro.props;
---

<Fragment set:html={contentRenderer(content, renderer)} />