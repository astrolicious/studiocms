{
  "extends": "astro/tsconfigs/strict",
  "include": ["**/*", "packages/studioCMS/**/*", "packages/studioCMSBlog/**/*"],
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "noEmit": false,
    "emitDeclarationOnly": true,
    "incremental": true,
    "noEmitOnError": true,
    "skipLibCheck": true,
    "outDir": ".moon/cache/types",
    "paths": {
      "@astrolicious/studiocms": ["packages/studioCMS/src/index.ts"],
      "@astrolicious/studiocms/*": ["packages/studioCMS/src/*"],
      "@astrolicious/studiocms-blog": ["packages/studioCMSBlog/index.ts"],
      "@astrolicious/studiocms-blog/*": ["packages/studioCMSBlog/src/*"]
    }
  },
  "references": [
    {
      "path": "packages/studioCMS"
    },
    {
      "path": "packages/studioCMSBlog"
    }
  ]
}
